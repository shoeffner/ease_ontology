<owl:ObjectProperty rdf:about="#hasPhase">
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
        <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#State"/>
        <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Process"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasConstituent"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasComponent"/>

<owl:ObjectProperty rdf:about="#hasProcessType">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
  <rdfs:range rdf:resource="#ProcessType"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRelatedToConcept"/>
  <owl:inverseOf rdf:resource="#isProcessTypeOf"/>
  <rdfs:comment>A relation between roles and process types, e.g. a catalysator is needed to trigger some chemical reaction.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRelatedToConcept"/>

<owl:ObjectProperty rdf:about="#hasQuale">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Region"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion"/>
  <owl:inverseOf rdf:resource="#isQualeOf"/>
  <rdfs:comment>Relates a quality to its "value", called quale, which is an atomic quality region.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isQualeOf">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Region"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Quality"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRegionFor"/>
  <rdfs:comment>Relates a quality to its "value", called quale, which is an atomic quality region.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasResult">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasShape">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:range rdf:resource="#Shape"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality"/>
  <owl:inverseOf rdf:resource="#isShapeOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isShapeOf">
  <rdfs:domain rdf:resource="#Shape"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isQualityOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSoftwareAgent">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasSpaceRegion">
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Localization"/>
        <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion"/>
  <owl:inverseOf rdf:resource="#isSpaceRegionFor"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isSpaceRegionFor">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion"/>
  <rdfs:range>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="#Localization"/>
        <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:range>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRegionFor"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasStateType">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Role"/>
  <rdfs:range rdf:resource="#StateType"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isRelatedToConcept"/>
  <owl:inverseOf rdf:resource="#isStateTypeOf"/>
  <rdfs:comment>A relation between roles and state types, e.g. 'the chair is the supporter of the person sitting on it'.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasStatus">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasQuality"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasTask">
  <rdfs:domain>
    <owl:Class>
      <owl:unionOf rdf:parseType="Collection">
        <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Relation"/>
        <rdf:Description rdf:about="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Workflow"/>
      </owl:unionOf>
    </owl:Class>
  </rdfs:domain>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
  <rdfs:comment>A relation to indicate that a Task is part of a Workflow or ordering Relation: that is, the task may be executed during the execution of the Workflow, or there exists some Relation between the Tasks that informs how their executions are to be located in time.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasTerminalState">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Transition"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  <rdfs:subPropertyOf rdf:resource="#includesSituation"/>
  <owl:inverseOf rdf:resource="#isTerminalStateOf"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isTerminalStateOf">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Transition"/>
  <rdfs:subPropertyOf rdf:resource="#isSituationIncludedIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#includesConcept">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#includesObject"/>
  <owl:inverseOf rdf:resource="#isConceptIncludedIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isConceptIncludedIn">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Concept"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isObjectIncludedIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isSituationIncludedIn">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasSetting"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#involvesArtifact">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalArtifact"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
  <owl:inverseOf rdf:resource="#isArtifactInvolvedIn"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artifact participation.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isArtifactInvolvedIn">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalArtifact"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Artifact participation.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#involvesEffector">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="#PhysicalEffector"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
  <owl:inverseOf rdf:resource="#isEffectorInvolvedIn"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Effector participation.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isEffectorInvolvedIn">
  <rdfs:domain rdf:resource="#PhysicalEffector"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Effector participation.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#involvesPlace">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasParticipant"/>
  <owl:inverseOf rdf:resource="#isPlaceInvolvedIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isPlaceInvolvedIn">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isParticipantIn"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAnsweredBy">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isAskedBy">
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isDepositOf">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf"/>
  <owl:inverseOf rdf:resource="#isOntopOf"/>
  <rdfs:comment>A spatial relation holding between an object (the deposit), and objects that are located ontop of it.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isOntopOf">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation"/>
  <rdfs:comment>A spatial relation holding between an object (the deposit), and objects that are located ontop of it.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isLinkedTo">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasLocation"/>
  <owl:inverseOf rdf:resource="#isLinkedTo"/>
  <rdfs:comment>A spatial relation holding between objects that are linked with each other such that they resist spatial separation.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isMovedByAgent">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  <rdfs:subPropertyOf rdf:resource="#interactsWith"/>
  <owl:inverseOf rdf:resource="#movesObject"/>
  <rdfs:comment>A relation from an object to an agent who causes it to move.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#movesObject">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
  <rdfs:subPropertyOf rdf:resource="#interactsWith"/>
  <rdfs:comment>A relation from an agent to an object that the agent causes to move.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isOccurrenceOf">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#EventType"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isClassifiedBy"/>
  <owl:inverseOf rdf:resource="#isOccurringIn"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between an event and an event type, e.g. 'taking the cup from the table' is an occurence of the motion 'approaching'.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is occurence of</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isOccurringIn">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#EventType"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A relation between an event and an event type, e.g. 'taking the cup from the table' is an occurence of the motion 'approaching'.</rdfs:comment>
  <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">is occurring in</rdfs:label>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isPerformedBy">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Action"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#involvesAgent"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isPhysicallyContainedIn">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isLocationOf"/>
  <rdfs:comment>A spatial relation holding between an object (the container), and objects it contains.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isPlanFor">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Plan"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Task"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#describes"/>
  <rdfs:comment>A special relation between a Plan and a Task, to indicate that the Plan describes a way to achieve the Task.</rdfs:comment>
  <rdfs:seeAlso>Note that DUL already says Plans can define Tasks, but that relation is used differently: a Plan defines a Task because it contains it as a step.</rdfs:seeAlso>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isSupportedBy">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  <rdfs:subPropertyOf rdf:resource="#interactsWith"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasCommonBoundary"/>
  <owl:inverseOf rdf:resource="#supports"/>
  <rdfs:comment>A relation between an object (the supporter) and another object (the supportee) where the supporter cancels the effect of gravity on the supportee.</rdfs:comment>
  <rdfs:comment>Relates a supportee to one of its supporters.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#supports">
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Entity"/>
  <rdfs:subPropertyOf rdf:resource="#interactsWith"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasCommonBoundary"/>
  <rdfs:comment>A relation between an object (the supporter) and another object (the supportee) where the supporter cancels the effect of gravity on the supportee.</rdfs:comment>
  <rdfs:comment>Relates a supportee to one of its supporters.</rdfs:comment>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#simultaneous">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#SymmetricProperty"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:subPropertyOf rdf:resource="#coOccurs"/>
  <owl:inverseOf rdf:resource="#simultaneous"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">`A simultaneous B` means that A strictly starts and ends at the same time instant as B, i.e. their temporal extend is equal.</rdfs:comment>
  <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">=</symbol>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#startedBy">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:subPropertyOf rdf:resource="#coOccurs"/>
  <owl:inverseOf rdf:resource="#starts"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">`A starts B` means that A starts exactly where B starts, and that A strictly ends before B. As in "I start my day with a coffee".</rdfs:comment>
  <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">si</symbol>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#starts">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
  <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Event"/>
  <rdfs:subPropertyOf rdf:resource="#coOccurs"/>
  <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">`A starts B` means that A starts exactly where B starts, and that A strictly ends before B. As in "I start my day with a coffee".</rdfs:comment>
  <symbol rdf:datatype="http://www.w3.org/2001/XMLSchema#string">s</symbol>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#transitionsBack">
  <rdfs:subPropertyOf rdf:resource="#transitionsFrom"/>
  <rdfs:subPropertyOf rdf:resource="#transitionsTo"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#transitionsTo">
  <rdfs:domain rdf:resource="#Transient"/>
  <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Object"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasCommonBoundary"/>
</owl:ObjectProperty>

