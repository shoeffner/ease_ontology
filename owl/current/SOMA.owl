</owl:Class>

<owl:Class rdf:about="#RedColor">
  <rdfs:subClassOf rdf:resource="#ColorRegion"/>
</owl:Class>

<owl:Class rdf:about="#Refrigerator">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasPhysicalComponent"/>
      <owl:someValuesFrom rdf:resource="#Door"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasPhysicalComponent"/>
      <owl:someValuesFrom>
        <owl:Class>
          <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="#Door"/>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#hasDisposition"/>
              <owl:onClass rdf:resource="#Coverage"/>
              <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#hasDisposition"/>
              <owl:onClass>
                <owl:Class>
                  <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="#Coverage"/>
                    <owl:Restriction>
                      <owl:onProperty rdf:resource="#affordsTrigger"/>
                      <owl:allValuesFrom>
                        <owl:Restriction>
                          <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
                          <owl:allValuesFrom rdf:resource="#Refrigerator"/>
                        </owl:Restriction>
                      </owl:allValuesFrom>
                    </owl:Restriction>
                  </owl:intersectionOf>
                </owl:Class>
              </owl:onClass>
              <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
            </owl:Restriction>
          </owl:intersectionOf>
        </owl:Class>
      </owl:someValuesFrom>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasDisposition"/>
      <owl:onClass rdf:resource="#Insertion"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasDisposition"/>
      <owl:onClass rdf:resource="#Tempering"/>
