  <rdfs:domain rdf:resource="#ShapeRegion"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  <rdfs:subPropertyOf rdf:resource="#hasShapeParameter"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasHeight">
  <rdfs:domain rdf:resource="#ShapeRegion"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  <rdfs:subPropertyOf rdf:resource="#hasShapeParameter"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasWidth">
  <rdfs:domain rdf:resource="#ShapeRegion"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  <rdfs:subPropertyOf rdf:resource="#hasShapeParameter"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasRadius">
  <rdfs:domain rdf:resource="#ShapeRegion"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  <rdfs:subPropertyOf rdf:resource="#hasShapeParameter"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasLength">
  <rdfs:domain rdf:resource="#ShapeRegion"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  <rdfs:subPropertyOf rdf:resource="#hasShapeParameter"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasFrictionValue">
  <rdfs:domain rdf:resource="#FrictionAttribute"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegionDataValue"/>
  <rdfs:comment>The coefficient of friction denotes the ratio of friction force between touching objects. The coefficient is dimensionless.</rdfs:comment>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasMassValue">
  <rdfs:domain rdf:resource="#MassAttribute"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#double"/>
  <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegionDataValue"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#hasFilePath">
