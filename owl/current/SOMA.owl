    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasDisposition"/>
      <owl:onClass rdf:resource="#Coverage"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#hasDisposition"/>
      <owl:onClass>
        <owl:Class>
          <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="#Coverage"/>
            <owl:Restriction>
              <owl:onProperty rdf:resource="#affordsTrigger"/>
              <owl:allValuesFrom rdf:resource="#Container"/>
            </owl:Restriction>
          </owl:intersectionOf>
        </owl:Class>
      </owl:onClass>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="#DesignedComponent"/>
</owl:Class>

<owl:Class rdf:about="#Pushing">
  <rdfs:subClassOf rdf:resource="#Actuating"/>
  <rdfs:comment>todo: define subclass 'PushingOver'? Would we expect two distinct contacts with the same surface then?</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#PhysicalEffector">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty>
        <rdf:Description>
          <owl:inverseOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasPart"/>
        </rdf:Description>
      </owl:onProperty>
      <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalAgent"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#PhysicalObject"/>
</owl:Class>

<owl:Class rdf:about="#LimbMotion">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isProcessTypeOf"/>
      <owl:someValuesFrom>
        <owl:Class>
          <owl:intersectionOf rdf:parseType="Collection">
            <rdf:Description rdf:about="#MovedObject"/>
            <owl:Restriction>
              <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#classifies"/>
              <owl:allValuesFrom rdf:resource="#Limb"/>
            </owl:Restriction>
          </owl:intersectionOf>
        </owl:Class>
      </owl:someValuesFrom>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="#DirectedMotion"/>
  <owl:disjointWith rdf:resource="#Locomotion"/>
  <owl:disjointWith rdf:resource="#PrehensileMotion"/>
</owl:Class>

<owl:Class rdf:about="#Linkage">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#affordsBearer"/>
      <owl:allValuesFrom rdf:resource="#LinkedObject"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#affordsTrigger"/>
      <owl:allValuesFrom rdf:resource="#LinkedObject"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="#Connectivity"/>
  <rdfs:comment>The disposition of an object (the linked object) to establish a connection with others by being linked together.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#LinkedObject">
  <rdfs:subClassOf rdf:resource="#ConnectedObject"/>
</owl:Class>

<owl:Class rdf:about="#LinkageState">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="#isStateTypeOf"/>
      <owl:someValuesFrom rdf:resource="#LinkedObject"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="#StateType"/>
  <rdfs:comment>Classifies States in which two objects are in a rigid connection, such that the movement of one determines the movement of the other.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#Localization">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion"/>
      <owl:someValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRegion"/>
      <owl:allValuesFrom rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#SpaceRegion"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="#Extrinsic"/>
  <rdfs:comment>The localization of an object. The region of this quality encodes values to localize the object in a dimensional space, e.g. Euclidean positions that localize the object in Euclidean space.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#SpatioTemporalRole">
  <rdfs:subClassOf rdf:resource="#EventAdjacentRole"/>
  <rdfs:comment>Roles that classify entities which locate an event or object in space and time.</rdfs:comment>
</owl:Class>

<owl:Class rdf:about="#SpatialRelationRole">
