      <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE.owl#DesignedHandle"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE.owl#hasPhysicalComponent"/>
      <owl:someValuesFrom rdf:resource="http://www.ease-crc.org/ont/EASE.owl#DesignedSpade"/>
    </owl:Restriction>
  </rdfs:subClassOf>
  <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#Cutlery"/>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/EASE.owl#DesignedSpade">
  <rdfs:subClassOf rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#DesignedComponent"/>
</owl:Class>

<owl:Class rdf:about="http://www.ease-crc.org/ont/EASE-OBJ.owl#SphereShape">
  <rdfs:subClassOf>
    <owl:Restriction>
      <owl:onProperty rdf:resource="http://www.ease-crc.org/ont/EASE-OBJ.owl#hasRadius"/>
      <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
      <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
    </owl:Restriction>
